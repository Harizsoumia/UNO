// CustomPanel.java
import javax.swing.*;
import java.awt.*;

public class CustomPanel extends JPanel {
    public CustomPanel() {
        setBackground(new Color(148, 0, 211));
    }
    
    public CustomPanel(LayoutManager layout) {
        super(layout);
        setBackground(new Color(148, 0, 211));
    }
}
